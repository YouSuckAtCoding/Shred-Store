{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProccessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "D:\\Projetos\\ShredStore\\ShredLog\\log.txt",
          "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exception:1}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5003"
        }
      }

    ]
  },
  "AllowedHosts": "*",
  "ApiUri": "https://localhost:7288",
  "PageLayouts": {
    "SecondaryLayout": "~/Views/Shared/SecondaryLayout.cshtml",
    "EmailLayout": "~/Views/Shared/EmailLayout.cshtml",
    "ResetPassword": "D:/Projetos/ShredStore/ShredStore/ShredStore/Views/EmailPages/ResetPasswordTemplate.cshtml",
    "AfterRegistration": "D:/Projetos/ShredStore/ShredStore/ShredStore/Views/EmailPages/AfterRegistrationTemplate.cshtml"
  },
  "ConnectionStrings": {
    "Redis": "localhost:5002"
  }
}
